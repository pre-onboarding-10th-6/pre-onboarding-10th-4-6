{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.26.1",
    "connected-react-router": "^6.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.1.0",
    "react-scripts": "^5.0.0",
    "styled-components": "^6.0.0-rc.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --cache \"./src/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --cache --write --config ./.prettierrc.json \"./src/**/*.{ts,tsx,js,jsx}\"",
    "postinstall": "husky install && git config --local commit.template .gitmessage.txt"
  },
  "eslintConfig": {
    "extends": "react-app"
